function checkStalker(npc_community, gulag_type, npc_rank, se_obj)
	if gulag_type=="lim_merc_dom" then
		return npc_community=="killer"
	end
	if gulag_type=="esc_private" or gulag_type=="age_mil_folls" or gulag_type=="mar_factory" or gulag_type=="esc_bridge" or gulag_type=="esc_blokpost" then
		return npc_community=="military"
	end
	if gulag_type=="esc_killers" or gulag_type=="esc_fabrika_bandit" or gulag_type=="red_near_bridge" or gulag_type=="red_scaut" or gulag_type=="lim_lager2_bandits"  then
		return npc_community=="bandit"
	end
	if gulag_type=="esc_lager" or gulag_type=="mar_lager" or  gulag_type=="lost_vil_stalker_dom" or gulag_type=="baza_mar_lager" or gulag_type=="esc_fox_camp" or gulag_type=="esc_assault" or  gulag_type=="esc_stalker_camp" or gulag_type=="esc_corps" or gulag_type=="bar_house" or gulag_type=="bar_stalker_disel" then
		return npc_community=="stalker"
	end
	if gulag_type=="dead_lager_botans" then
		return npc_community=="ecolog"
	end
	if gulag_type=="marsh_baze_ecolog" or gulag_type=="marsh_base_botan_lager" then
		return npc_community=="ecolog" or npc_community=="stalker" or npc_community=="green"
	end
	if gulag_type=="rad_merc" or gulag_type=="rad_merc_base" or gulag_type=="esc_vlad" then
		return true
	end
	if gulag_type=="red_forest_exit_mil_2" then
		return npc_community=="stalker" or npc_community=="freedom"
	end
	if gulag_type=="esc_dram" then
		return npc_community=="green"
	end
	if gulag_type=="esc_specnaz" then
		return se_obj:profile_name()=="esc_soldier_specnaz"
	end
	return false
end

function checkMonster(npc_community, gulag_type)
	if gulag_type=="esc_dogs_to_fox" or gulag_type=="esc_dogs_swarm" or gulag_type=="esc_small_home3" or gulag_type=="esc_small_home4" or gulag_type=="esc_small_home5" then
		return npc_community=="dog"
	end
	if gulag_type=="esc_boars_dogs" then
		return npc_community=="dog" or npc_community=="boar"
	end
	if gulag_type=="esc_zombie_1" or gulag_type=="esc_zombie_2" then
		return npc_community=="zombie"
	end
	if gulag_type=="esc_flesh_1" or gulag_type=="esc_flesh_2" or gulag_type=="esc_small_home6" then
		return npc_community=="flesh"
	end
	if gulag_type=="esc_bridge_boar" or gulag_type=="esc_small_home1" or gulag_type=="esc_small_home2" then
		return npc_community=="boar"
	end
	return false
end