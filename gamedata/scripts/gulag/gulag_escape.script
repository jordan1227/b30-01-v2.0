["baza_mar_lager"]=true
}
function load_states(gname, type)
	
	if simpl_type[type] then
		return function(gulag)
			return 0
		end
	end

	if type=="esc_blokpost" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end

			for k,v in pairs(gulag.Object) do
				if v~=true and v:best_enemy()~=nil and v:best_enemy():id()==actor:id() then
					if not has_alife_info("escape_lager_help") then
					gulag.alarm_start = game.get_game_time()
					--actor:give_info_portion("mil_find_prizrak_today")
					return 2
					end
				end
			end

			if gulag.alarm_start==nil or game.get_game_time():diffSec(gulag.alarm_start) > 300 then
				if (level.get_time_hours()*60+level.get_time_minutes()) >= 380 and (level.get_time_hours()*60+level.get_time_minutes()) < 1345 then
				actor:give_info_portion("esc_blockpost_normal")
					return 0
				else
					return 1
				end
			else
				return 2
			end
		end
	end
	
	if type=="esc_fox_camp" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end
			local vrem=level.get_time_hours()
			if (vrem >= 5 and vrem < 8) or (vrem >= 10 and vrem < 12) or(vrem >= 15 and vrem < 18) or(vrem >= 21 and vrem < 22) then
				return 0
			else
				return 1
			end
		end
	end
	if type=="rad_merc" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end
			if has_alife_info("volkodav_rad_ok") then
				return 0
			else
				return 1
			end
		end
	end

	if type=="esc_lager" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end
			if has_alife_info("escape_lager_help") and (not has_alife_info("escape_lager_killers_die")) then
				return 2
			elseif (level.get_time_hours()*60+level.get_time_minutes()) >= 320 and (level.get_time_hours()*60+level.get_time_minutes()) < 1355 then
				return 0
			else
				return 1
			end
		end
	end

	if type=="esc_bridge" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end
			local shift=3
			local holidays=level.get_time_days()
			for i=1,5 do
				if (holidays >= shift*(i*2-1) and holidays < shift*(i*2)) then
				return 1
				end
			end
				return 0
		end
	end
	
	if type=="esc_dram" then
		return function(gulag)
			if db.actor~=nil then
				if (level.get_time_hours()*60+level.get_time_minutes()) >= 360 and (level.get_time_hours()*60+level.get_time_minutes()) < 1355 then
					return 0
				else
					return 1
				end
			else
				return gulag.state
			end
		end
	end
	
	if type=="esc_fabrika_bandit" then
		return function(gulag)
			if not has_alife_info("atp_bandits_open_fire") then
				return 0
			else
				for k,v in pairs(gulag.Object) do
					if v~=true and v:best_enemy()~=nil then
							gulag.alarm_start = game.get_game_time()
							return 2
					end
				end

				if gulag.alarm_start==nil or
					game.get_game_time():diffSec(gulag.alarm_start) > 300 then
					return 0
				else
					return 2
				end
			end
		end
	end

	if type=="esc_dogs_to_fox" then
		return function(gulag)
			local actor = db.actor
			if not has_alife_info("escape_stalker_dog_spawn") then
				return 0
			elseif (has_alife_info("escape_stalker_dog_spawn") and not has_alife_info("escape_stalker_done")) or has_alife_info("esc_dogs_return") or has_alife_info("esc_dogs_stay_home") then 
				return 2
			else
				return 1
			end
		end
	end

	if type=="esc_specnaz" then
		return function(gulag)
			local actor = db.actor
			if has_alife_info("esc_raid1_start") then
				return 1
			end
			return 0
		end
	end

	if type=="esc_boars_dogs" then
			return function(gulag)
			if has_alife_info("tutorial_wounded_give_info") then
				return 1
			else
				return 0
			end
		end
	end

	if type=="esc_killers" then
		return function(gulag)
			local actor = db.actor
			if not actor then
				return gulag.state
			end
			if not has_alife_info("escape_lager_spawn_killers") then
				return 0
			elseif has_alife_info("escape_lager_spawn_killers") and not has_alife_info("esc_killers_raid") then
				return 3
			elseif has_alife_info("esc_killers_raid") and not has_alife_info("esc_open_killer_fire") then
				return 4
			elseif has_alife_info("esc_open_killer_fire") and xr_gulag.getGulagPopulationComed("esc_lager") > 0 then
				return 1
			elseif has_alife_info("esc_open_killer_fire") and xr_gulag.getGulagPopulationComed("esc_lager") <= 0 then
				return 2
			end
			return gulag.state
		end
	end

	if type=="esc_dogs_swarm" then
		return function(gulag)
			if game.get_game_time():diffSec(gulag.stateBegin) > 900 then
				if gulag.state==0 then
					return 1
				elseif gulag.state==1 then
					return 2
				elseif gulag.state==2 then
					return 0
				end
			else
				return gulag.state
			end
		end
	end

	if type=="esc_corps" then
		return function(gulag)
			if has_alife_info("esc_kill_bandits_quest_have") then
				return 1
			else
				return 0
			end
		end
	end

	if type=="esc_small_home3" then
		return function(gulag)
			if db.actor~=nil then
				if (level.get_time_hours() >= 5 and level.get_time_hours() <= 22) then
					return 0
				else
					return 1
				end
			else
				return gulag.state
			end
		end	
	end

	if type=="esc_stalker_camp" then
		return function(gulag)
			if has_alife_info("yan_provodnik_spawn") or has_alife_info("tesla_in") then
				return 1
			else
				return 0
			end
		end
	end

	if type=="esc_assault" then
		return function(gulag)
			local ret=0
			if has_alife_info("esc_kill_bandits_quest_have") and not has_alife_info("esc_assault_start") and not has_alife_info("esc_kill_bandits_quest_done") then
				ret=1
			elseif has_alife_info("esc_assault_start") and not has_alife_info("esc_kill_bandits_quest_kill") then
				ret=2
			elseif has_alife_info("esc_kill_bandits_quest_kill") and(not has_alife_info("esc_kill_bandits_quest_done") or not has_alife_info("esc_petruha_great")) then
				ret=3	
			elseif has_alife_info("esc_kill_bandits_quest_done") or has_alife_info("esc_petruha_great") then
				ret=4
			else
				ret=0
			end
			return ret
		end
	end

end
